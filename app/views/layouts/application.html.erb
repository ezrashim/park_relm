<!DOCTYPE html>
<html>
<head>
  <title>ParkRelm</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <section class='top-bar-section'>
    <ul class='right'>
      <% if current_user %>
        <li> <%= link_to 'Sign Out','#' %></li>
      <% end %>
    </ul>
  </section>


  <% flash.each do |key, value| %>
    <div>
      <%= value %>
    </div>
  <% end %>

<%= yield %>

</body>
</html>
